<h1>Bandwidths</h1>

<table>
  <thead>

    <tr>
      <th>Server id</th>
      <th>Value</th>
      <th>Interface name</th>
      <th colspan="3"></th>
    </tr>

  </thead>

  <%@server.bandwidths.each do |bandwidth| %>
    
    <tr>
      <td><%= bandwidth.server_id %></td>
      <td><%= bandwidth.value %></td>    
      <td><%= bandwidth.interface_name %></td>
    </tr>

  <% end %>

</table>
